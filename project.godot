config_version=5

[application]

config/name="idle-rpg"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://icon.svg"

[autoload]

ResourceManager="*res://Utilities/Resources/resource_manager.gd"

[display]

window/size/viewport_width=360
window/size/viewport_height=640
window/stretch/mode="canvas_items"
window/handheld/orientation=1

[dotnet]

project/assembly_name="hunter-idle-rpg"

[file_customization]

folder_colors={
"res://Assets/": "yellow",
"res://Common/": "green",
"res://Entities/": "red",
"res://Utilities/": "purple"
}

